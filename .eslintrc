{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "document": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "import/extensions": "off",
    "no-underscore-dangle": "off",
    "arrow-body-style": "off"
  },
  "extends": "airbnb"
}
